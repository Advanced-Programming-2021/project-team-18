<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import java.net.URL?>
<BorderPane prefHeight="600.0" prefWidth="860.0" styleClass="mainMenuPane" xmlns="http://javafx.com/javafx/11.0.2"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.menu.mainmenu.MainMenuView">
    <stylesheets>
        <URL value="@../CSS/styles.css"/>
    </stylesheets>
    <bottom>
        <VBox alignment="CENTER" spacing="30">
            <Button mnemonicParsing="false" prefHeight="39.0" prefWidth="112.0" text="logout" onMouseClicked="#onLogoutButton"/>
            <Button onMouseClicked="#enterShopMenu" prefHeight="57.0" prefWidth="165.0" text="shop menu"/>
            <HBox alignment="CENTER" spacing="30">
                <Button onMouseClicked="#enterImportExportMenu" prefHeight="57.0" prefWidth="165.0"
                        text="import/export menu"/>
                <Button onMouseClicked="#enterDeckMenu" prefHeight="57.0" prefWidth="165.0" text="deck menu"/>
                <Button onMouseClicked="#enterDuelMenu" prefHeight="57.0" prefWidth="165.0" text="duel menu"/>
            </HBox>
            <HBox alignment="CENTER" spacing="30">
                <Button onMouseClicked="#enterScoreboardMenu" prefHeight="57.0" prefWidth="165.0"
                        text="scoreboard menu"/>
                <Button onMouseClicked="#enterProfileMenu" prefHeight="57.0" prefWidth="165.0" text="profile menu"/>
                <Button onMouseClicked="#enterCardCreatorMenu" prefHeight="57.0" prefWidth="165.0"
                        text="card creator menu"/>
            </HBox>
        </VBox>
    </bottom>
</BorderPane>
